<!DOCTYPE html>
<html class="has-navbar-fixed-top" lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>WikiMap</title>
    <!-- stylesheet includes Bulma for resets and framework -->
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
    <!-- Load JS scripts -->
    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
    <script type="text/javascript" src="/scripts/load-functions.js"></script>
    <script type="text/javascript" src="/scripts/map-functions.js"></script>
    <script type="text/javascript" src="/scripts/interfaceManipulation.js"></script>
    <!-- Script Tag for connection to Google Maps API -->
    <script id="map_script"
      src="https://maps.googleapis.com/maps/api/js?key=<%=process.env.MAP_API_KEY%>&libraries=&v=weekly"
      defer
    ></script>
  </head>
  <body class="column is-gapless mt-3">
    <%- include('partials/_nav') %>
    <!-- all views are in a column and hidden/shown as needed -->
    <%- include('partials/_login_reg_view') %>
    <%- include('partials/_profile_view') %>
    <%- include('partials/_map_view') %>
    <%- include('partials/_list_view') %>
  </body>
</html>
